@startuml MeninasDomain
!theme reddress-darkred
title Diagrama de Clases de un Cuadro Naturalista

class Picture {
    + Title, Year, Size...
    + Aesthetic: naturalistic
    + Context
    + Exhibited
}
Picture *-- Author
Picture o--"0..n" Viewer
Picture *-- Narrative

abstract class Narrative {
    + Action
}
Narrative <|-- Denotative
Narrative <|-- Descriptive

class Denotative {
}

Denotative *-- Role
Denotative *-- Model

class Descriptive {
}
Descriptive *-- Scene

class Scene {
    + lighting
    + boundaries
}
Scene *--"1..n" Ground

class Ground {
    + Area
}
Ground <|-- Foreground
Ground <|-- Background
Ground <|-- Imaginary

Ground o--"0..n" Character
Ground o--"0..n" SetElement


class SetElement {
   + Position
}

class Character {
    + Position
    + toObserve(other_position)
}
Character ..> Model
Character ..> Role
SetElement ..> Model

class Viewer {
}

@end